% Create a HMM model
% SP - start probability
% A - transition matrix
% B - Emission
%
% Note: for each state s in S and t in T
% the items should follow the condition
% sum  SP(s) = 1
% sum  A(s1, s2) = 1
% sum  B(s, t) = 1

% number of states
N = 7;

%number of features on observtion vector
M = 2;

Aguess = ones(N)/N;
Bguess = ones(N,M)/M;

[A, B] = hmmtrain( , Aguess, Bguess);

